INSERT INTO `colorboxes` (`produto_id`, `titulo`, `sku`, `descricao`, `valor`) VALUES (2162,'Colorbox Alegria','alegria','Tour cultural, peças de teatro, cinema, paintball, passeio de bicicleta, workshop de musica, rafiting Day, batismo de yoga, aula de circo, kart, massagem relaxante, trilhas incríveis, snorkeling, bóio cross, arvorismo, escalada, batismo de surf, visita a aquário, snooker com os amigos.\r\nEscolha a experiência que mais lhe agrada e torne a vida de alguém ainda mais especial.','49.00'),(2163,'Colorbox Diversão','diversao','Conheça e experimente as belezas naturais do nosso planeta que só O Melhor da Vida traz para você. Aproveite o que a biodiversidade e as maravilhas da nossa terra têm à oferecer. Respire fundo o ar puro, sinta a energia do sol e o perfume das flores. Você merece.','189.00'),(2164,'Colorbox Emoções','emocoes','Aula de pilates, vôo de trike, curso de perfumaria, degustação de vinhos, jantar romântico, aula de golf, esgrima, aula de dança de salão, programa de fitness, arco e flecha, passeio em catamaran, tênis, noite no boliche, mixe de aventura, iniciação ao montanhismo,  astronomia, off Road e mountain bike com sua turma.\r\nDesperte suas sensações e desejos !!!','99.00'),(2165,'Colorbox Sensações','sensacoes','Personal shopper,programa de beleza e bem estar, mixe de terapia, aula de windsurf, golf Day, workshop de maquiagem, degustação de café e chocolates, jantar multisensorial, curso de caipirinha, quiropraxia, pousas de charme, vôo duplo, aula de wakeboard, fototerapia, batismo de yoga, off Road, workshop de temaki sushi e sashimi, eletrolifting, jantar afrodisíaco e mais ....\r\nAtive as mais incríveis sensações do seu corpo com nossas experiências!','249.00'),(2166,'Colorbox Encantamento','encantamento','Vôo de balcão, final de semana temático - relaxante, gourmet, radical, gastronômico, banho de chocolate, observação de pássaros, batismo de kitesurf, pousadas sofisticadas, vôo acrobático, jantares luxuosos, curso de culinária, passeio de lancha, Jet-ski, quadricículo, batismo de mergulho, um dia na fazenda, ritual de massagem, auto maquiagem, curso de coquetelaria....','399.00'),(2167,'Colorbox Sonhos','sonhos','Pousadas em ilhas paradisíacas, fazendas históricas, praias desertas, noites românticas, menu degustação nos melhores restaurantes do pais, vôo panorâmico, curso de mergulho, salto de pára-quedas, nadando com tubarões, Day SPA sofisticado, vôo de balão,personal trainer, pesca com os amigos, curso de bartender, personal diet...','599.00'),(2168,'Colorbox O Melhor Da Vida','o-melhor-da-vida','Hotéis de luxo e sofisticados, resorts, SPA, golf, thermas, aluguel de veleiros, Harley davidson, bangalôs ecológicos, chefe em sua casa, workshop de DJ, curso de barista, cavernas do Petar, jantares intimistas com hospedagem e mais...','999.00');
